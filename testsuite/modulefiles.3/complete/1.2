#%Module

module-whatis [module-info name]

if {[info exists env(TESTSUITE_COMPLETE)]} {
    switch -- $env(TESTSUITE_COMPLETE) {
        compbreak1 {
            complete bash othercmd {-o default -F _othercmd}
        }
        compcomb1 {
            uncomplete cmd
        }
        uncompbreak1 {
            uncomplete othercmd
        }
        uncompcomb1 {
            complete bash cmd {-o default -F _cmd}
        }
    }
}

