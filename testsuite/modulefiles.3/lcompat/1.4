#%Module

module-whatis [module-info name]

if {[info exists env(TESTSUITE_LCOMPAT)]} {
    switch -- $env(TESTSUITE_LCOMPAT) {
        reqfull1 - reqfull2 {
            require-fullname
        }
        reqfull3 {
            variant --default val2 foo val1 val2 val3
            require-fullname
        }
        reqfull4 {
            require-fullname string
        }
    }
}

