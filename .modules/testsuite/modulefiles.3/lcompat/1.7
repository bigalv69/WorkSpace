#%Module

module-whatis [module-info name]

if {[info exists env(TESTSUITE_LCOMPAT)]} {
    switch -- $env(TESTSUITE_LCOMPAT) {
        allo1 {
            always-load foo/1.0
        }
        allo2 {
            always-load foo/1.0 bar/1.0
        }
        allo3 {
            always-load foo@1 bar@:1
        }
        allo4 {
            always-load foo/0.err bar/1.0
        }
        allo5 {
            always-load foo/1.0 unk/1.0
        }
        allo6 {
            always-load
        }
        allo7 {
            always-load foo bar
        }
    }
}

