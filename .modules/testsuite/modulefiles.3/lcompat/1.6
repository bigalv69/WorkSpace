    switch -- $env(TESTSUITE_LCOMPAT) {
        prall1 {
            prereq-all foo/1.0
        }
        prall2 {
            prereq-all foo/1.0 bar/1.0
        }
        prall3 {
            prereq-all foo@1 bar@:1
        }
        prall4 {
            prereq-all foo/0.err bar/1.0
        }
        prall5 {
            prereq-all foo/1.0 unk/1.0
        }
        prall6 {
            prereq-all
        }
    }
}

