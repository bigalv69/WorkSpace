#%Module

module-whatis [module-info name]

if {[info exists env(TESTSUITE_COMPLETE)]} {
    switch -- $env(TESTSUITE_COMPLETE) {
        uncompbad1 {
            uncomplete
        }
        uncompbad2 {
            uncomplete cmd cmd
        }
        uncompbad3 {
            uncomplete {}
        }
        uncomp1 {
            uncomplete cmd
            uncomplete cmd
            uncomplete othercmd
        }
        uncompbreak1 - uncompbreak2 {
            uncomplete cmd
            break
        }
        uncompcomb1 - comp3 {
            uncomplete cmd
        }
    }
}

