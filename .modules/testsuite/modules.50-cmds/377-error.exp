set ans [list]
lappend ans [list set __MODULES_LMPREREQ rec&rea|reb:red&rea|reb:ree&rea|reb]
lappend ans [list set _LMFILES_ $mp/rea:$mp/reb:$mp/rec:$mp/red:$mp/ree]
