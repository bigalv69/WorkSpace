testouterr_cmd_re sh {load reb} $ans [join $tserr \n]
set ans [list]
lappend ans [list set LOADEDMODULES rea:reb:rei:rek]
lappend ans [list set _LMFILES_ $mp/rea:$mp/reb:$mp/rei:$mp/rek]
