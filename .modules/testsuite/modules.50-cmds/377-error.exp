#
# load module
#

# crash of a ReqLo module (independent ReqLo, middle one crashes)
unsetenv_loaded_module
unsetenv_var __MODULES_LMPREREQ
unsetenv_var __MODULES_LMCONFLICT

set ans [list]
lappend ans [list ERR]
set tserr [list {load rad} {load raa} {load rab} \n[msg_load rab $err_evalabort] \n[msg_load rad [err_reqlo rab]]]
testouterr_cmd_re sh {load rad} $ans [join $tserr \n]
set ans [list]
lappend ans [list set __MODULES_LMPREREQ rad&raa&rab&rac]
lappend ans [list set LOADEDMODULES raa:rac:rad]
lappend ans [list set _LMFILES_ $mp/raa:$mp/rac:$mp/rad]
lappend ans [list set __MODULES_LMTAG raa&auto-loaded:rac&auto-loaded]
