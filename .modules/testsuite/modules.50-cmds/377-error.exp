lappend ans [list set __MODULES_LMPREREQ ref&rea|reb:reg&rea|reb&ref:reh&rea|reb&reg]
lappend ans [list set _LMFILES_ $mp/rea:$mp/reb:$mp/ref:$mp/reg:$mp/reh]
